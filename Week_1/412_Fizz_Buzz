class Solution:
    def fizzBuzz(self, n: int) -> List[str]:
        ans = []
        for i in range(1,n+1):
            if i % 15 == 0:
                ans.append("FizzBuzz")
            elif i % 3 == 0:
                # _3list[i] = 1
                ans.append("Fizz")
            elif i%5 == 0:
                # _5list[i] = 1
                ans.append("Buzz")
            else:
                ans.append(str(i))
        return ans